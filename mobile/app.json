{
  "expo": {
    "name": "Bilet Buldum",
    "slug": "bilet-buldum",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/biletbuldum-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.furkan-gunduz.biletbuldum",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-3940256099942544~1458002511"
      },
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you and improve your ticket search experience.",
        "NSLocationWhenInUseUsageDescription": "We need your location to show you relevant ticket events near you.",
        "NSCameraUsageDescription": "We need camera access to let you scan tickets and upload profile pictures.",
        "NSPhotoLibraryUsageDescription": "We need photo library access to let you upload profile pictures.",
        "NSNotificationsUsageDescription": "We'll send you notifications about ticket availability and updates on your saved searches."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/biletbuldum-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.furkan_gunduz.biletbuldum",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-3940256099942544~3347511713"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/biletbuldum-icon.png",
          "color": "#ffffff",
          "mode": "production"
        }
      ],
      "react-native-google-mobile-ads"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "notification": {
      "icon": "./assets/biletbuldum-icon.png",
      "color": "#ffffff",
      "androidMode": "default",
      "androidCollapsedTitle": "New Notification",
      "iosDisplayInForeground": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6a2e4476-24ab-46d1-8dba-9b6ffab7b2e1"
      }
    }
  }
}
